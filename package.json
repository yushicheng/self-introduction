{
  "name": "self-introduction",
  "description": "基于docusaurus制作的文档网站",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "dev": "docusaurus start",
    "build": "docusaurus build && gh-pages -d build",
    "export": "mr-pdf --pdfFormat=A2 --outputPDFFilename=./static/个人简历.pdf --pdfMargin=20 --excludeSelectors=\"footer,aside\" --paginationSelector=\".pagination-nav__item--next > a\" --initialDocsURL=\"http://localhost:3000/self-introduction/docs/自我介绍\"",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.8",
    "@docusaurus/preset-classic": "2.0.0-beta.8",
    "@docusaurus/theme-live-codeblock": "2.0.0-beta.8",
    "@mdx-js/react": "^1.6.21",
    "@svgr/webpack": "^5.5.0",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "file-loader": "^6.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "url-loader": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "mdx-mermaid": "^1.1.0",
    "mermaid": "^8.13.3",
    "mr-pdf": "^0.2.3"
  }
}